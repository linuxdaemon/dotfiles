{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "platform": "github",
  "labels": ["update"],
  "prConcurrentLimit": 5,
  "packageRules": [
    {
      "matchManagers": ["github-actions"],
      "pinDigests": true,
      "commitMessageTopic": "github-actions",
      "groupName": "github-actions",
      "automerge": false
    }
  ]
}
